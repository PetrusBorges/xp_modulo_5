generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid()) @db.Uuid
  name     String
  email    String  @unique
  age      Int
  isActive Boolean @default(true) @map("is_active")

  @@map("users")
}
